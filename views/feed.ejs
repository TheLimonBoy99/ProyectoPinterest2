<body>

    <nav>    
        <div class="pinterest">
          <div class="left">
            <img src="https://i.pinimg.com/originals/d3/d1/75/d3d175e560ae133f1ed5cd4ec173751a.png" alt="pin logo" class="img1" />
          </div>
          <div class="search">
            <i class="fas fa-search"></i>
            <input type="search" name="" placeholder="Search" id="">
          </div>
          <div class="right">
            <a href="/profile" class="avatar">
              <div class="img"><img src="https://images.unsplash.com/photo-1513721032312-6a18a42c8763?w=152&h=152&fit=crop&crop=faces" alt=""></div>
            </a>
            <a href="/login" id="salir"><i class="fas fa-sign-out-alt"></i></a>
          </div>
        </div>
   </nav>
 

<div class="container">
     
    <div class="gallery">
        <% posts.forEach(post => { %>
            <div class="box">
                <a href="<%= post.image %>">
                    <img src="<%= post.image %>" alt="image">
                </a>
                <div class="gallery-item-info">
                    <ul>
                        <li class="gallery-item-likes"><span class="visually-hidden">Likes:</span><i class="fas fa-heart" aria-hidden="true"></i> <%= post.likes.length %></li>
                        <!-- Agregar mÃ¡s detalles del post si es necesario -->
                    </ul>
                </div>
                <div class="caption"><%= post.imageText %></div>
            </div>
        <% }); %>
    </div>
     
